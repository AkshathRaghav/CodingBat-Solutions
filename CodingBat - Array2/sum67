sum67 

public int sum67(int[] nums) {
  int count = 0 ; 
  boolean check = false ; 
  for (int i = 0 ; i < nums.length ; i++) {
    if (!check) { 
      if ( nums[i] == 6) { 
        check = true ; 
      } 
      else { 
        count += nums[i] ; 
      } 
    } 
    
    if ( nums[i] == 7 ) { 
      check = false ; 
    }
     
  }
  return count ; 
    
}
