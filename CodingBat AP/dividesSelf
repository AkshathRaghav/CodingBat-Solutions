dividesSelf - 

public boolean dividesSelf(int n) {
  boolean check = false ; 
  int n0 = n , temp; 
  while ( n0 > 0 ) { 
    temp = n0%10  ; 
    if( temp == 0 ) { 
      return false ; } 
    if ((n % temp == 0)) { 
      check = true  ; 
    } 
    else { return false ; } 
    n0 /= 10 ; 
    
    if ( check == false ) { 
      return false ; } 
      
  } 
  return check  ; 
  
}
