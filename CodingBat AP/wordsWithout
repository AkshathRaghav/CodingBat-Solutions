wordsWithout - 

public String[] wordsWithout(String[] words, String target) {
  
  int count = 0 ; 
  for (String i : words ) { 
    if (!(i.equals(target))) { 
      count++ ; 
    } 
  } 
  String[] check = new String[count] ; 
  int j = 0 ; 
  for (int i = 0 ; i < words.length ; i++ ) { 
    if (!(words[i].equals(target))) { 
      check[j] = words[i]  ; 
      j++; 
    } 

  }
  return check ; 
  
}
